<p id="notice"><%= notice %></p>

<h1>Ulicas</h1>

<table>
  <thead>
    <tr>
      <th>Id ulicy</th>
      <th>Nazwa ulicy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ulicas.each do |ulica| %>
      <tr>
        <td><%= ulica.id_ulicy %></td>
        <td><%= ulica.nazwa_ulicy %></td>
        <td><%= link_to 'Show', ulica %></td>
        <td><%= link_to 'Edit', edit_ulica_path(ulica) %></td>
        <td><%= link_to 'Destroy', ulica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ulica', new_ulica_path %>
