<p id="notice"><%= notice %></p>

<h1>Obszars</h1>

<table>
  <thead>
    <tr>
      <th>Id obszaru</th>
      <th>Kod pocztowy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @obszars.each do |obszar| %>
      <tr>
        <td><%= obszar.id_obszaru %></td>
        <td><%= obszar.kod_pocztowy %></td>
        <td><%= link_to 'Show', obszar %></td>
        <td><%= link_to 'Edit', edit_obszar_path(obszar) %></td>
        <td><%= link_to 'Destroy', obszar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Obszar', new_obszar_path %>
